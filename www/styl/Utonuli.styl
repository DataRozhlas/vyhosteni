.utonuli
  width 100%
  height 550px
  margin 30px 0px
  border-top 1px solid #b3
  border-bottom 1px solid #b3
  background white
  .leaflet-popup-pane
    pointer-events none
    .leaflet-popup-close-button
      display none
    .leaflet-popup-content-wrapper
      box-shadow 0px 3px 7px rgba(0,0,0,0.8)
      border-radius 5px
    .leaflet-popup-content
      margin 10px 15px
